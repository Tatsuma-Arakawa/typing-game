rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if
      request.resource.data.name is string &&
      request.resource.data.name != null &&
      request.resource.data.name > 0 &&
      request.resource.data.name < 11 &&
      request.resource.data.mode is string &&
      request.resource.data.mode != null &&
      request.resource.data.rank is string &&
      request.resource.data.rank != null &&
      request.resource.data.score is int &&
      request.resource.data.score != null;
    }
  }
}